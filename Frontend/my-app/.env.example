# Environment Configuration
# Copy this file to .env.local and update the values as needed

# Environment: dev | production
ENV=dev

# Port for the frontend server (must be between 1-65535)
PORT=3001

# Backend server URL (must be a valid URL)
SERVER_URL=http://localhost:3000

# Optional: Next.js public environment variables (prefixed with NEXT_PUBLIC_)
# These will be available in the browser
NEXT_PUBLIC_ENV=dev
